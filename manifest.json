{
    "$schema": "https://schema.showpad.com/app/manifest.v2.json",
    "schema": {
        "type": "app-manifest",
        "version": 2
    },
    "appKey": "<your-app-key>",
    "name": "Quickstart App",
    "version": "0.0.1",
    "description": {
        "short": "The Quickstart App",
        "full": "This App can be used as a template to start making your own App"
    },
    "developer": {
        "name": "<your-developer-name>",
        "websiteUrl": "<your-website-url>"
    },
    "icon": "images/showpad-logo.jpg",
    "images": ["images/showpad-logo.jpg"],
    "adminSettings": [
        {
            "extensionKey": "admin-settings",
            "name": "Quickstart Admin Settings",
            "description": "My admin settings extensions",
            "resources": {
                "folder": "admin-settings"
            }
        }
    ],
    "userSettings": [
        {
            "extensionKey": "user-settings",
            "name": "Quickstart User Settings",
            "description": "My user settings extensions",
            "resources": {
                "folder": "user-settings"
            }
        }
    ],
    "experienceTypes": [
        {
            "extensionKey": "experience-type",
            "name": "Quickstart Experience",
            "description": "My experience types",
            "resources": {
                "folder": "experience"
            }
        }
    ]
}
